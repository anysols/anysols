{
  "name": "anysols",
  "version": "0.0.12",
  "description": "Sleek and powerful platform for faster and easier application development.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "mongod": "mongod",
    "test": "nyc mocha --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "extension": [
      ".js"
    ],
    "include": [
      "**/!(*.spec.*).js"
    ],
    "exclude": [
      "assets/",
      "docs/",
      "bin/",
      "**/gulpfile.js",
      "test/"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "docs/reports/coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anysols/anysols.git"
  },
  "bin": {
    "anysols": "./bin/anysols.js"
  },
  "author": "Shivaji Varma <contact@shivajivarma.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/anysols/anysols/issues"
  },
  "homepage": "https://anysols.com",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "copy-dir": "^0.3.0",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.8",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.4.0",
    "express-winston": "^2.5.0",
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.15",
    "mongoose-schema-extend": "^0.2.2",
    "mysql2": "^1.5.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.5.1",
    "sequelize": "^4.36.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "selenium-webdriver": "^4.0.0-alpha.1"
  }
}
