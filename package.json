{
    "name": "anysols",
    "version": "2.0.0",
    "description": "Sleek and powerful platform for faster and easier application development.",
    "main": "lib/app.js",
    "scripts": {
        "start": "node ./lib/app.js",
        "build": "gulp | tsc",
        "test": "nyc mocha --recursive",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "local_install": "npm install -g .",
        "objectid": "node bin/objectid.js"
    },
    "nyc": {
        "check-coverage": false,
        "all": true,
        "extension": [
            ".js"
        ],
        "include": [
            "**/!(*.spec.*).js"
        ],
        "exclude": [
            "assets/",
            "docs/",
            "bin/",
            "**/gulpfile.js",
            "test/",
            "playground.js"
        ],
        "reporter": [
            "html",
            "lcov",
            "text",
            "text-summary"
        ],
        "report-dir": "docs/reports/coverage"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/anysols/anysols.git"
    },
    "bin": {
        "anysols": "./bin/anysols.js"
    },
    "author": "Shivaji Varma <contact@shivajivarma.com>",
    "contributors": [],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://gitlab.com/anysols/anysols/issues"
    },
    "homepage": "https://anysols.com",
    "dependencies": {
        "anysols-core-service": "^0.7.5",
        "anysols-server-service": "^0.7.5",
        "anysols-utils": "^0.2.0",
        "commander": "^2.16.0",
        "cron": "^1.3.0",
        "dotenv": "^5.0.1",
        "istanbul": "^0.4.5",
        "json-stringify-safe": "^5.0.1",
        "jsonwebtoken": "^8.2.0",
        "memory-cache": "^0.2.0",
        "moment": "^2.22.2",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "winston": "^2.4.1"
    },
    "devDependencies": {
        "gulp": "^4.0.2",
        "chai": "^4.2.0",
        "del": "^5.1.0",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "objectid": "^3.2.1",
        "typescript": "^3.6.2",
        "uuid": "^3.2.1"
    }
}
