<% modules.forEach(function(module, index) { %>
<li class="">
    <a class="<% if(module.children) { %>shortcuts-tree<% } else {%>link<%}%>" href="javascript:void(0)" style="position:relative">
        <span class="nav-item-name"><%= module.name %></span>
        <% if(module.children) { %>
            <i class="fas fa-chevron-down" style="position: absolute; right: 10px;"></i>
        <% } %>
    </a>
    <% if(module.children && module.children.length) { %>
    <ul class="sidebar-sub-level-items fly-out-list" style="">
        <li class="divider fly-out-top-item"></li>
        <%- partial('./nav-tree', {modules: module.children}) %>
    </ul>
    <% } %>
</li>
<% }); %>